INSERT INTO public.expedition (expedition_id, name, description, avatar_small, avatar_full, costs, current_sum, stage_id, banned, route_plan, route_current, head) VALUES (1, 'Поиск Йети на Марсе', 'Команда исключительно весёлых энтузиастов в лице Альберта и Семёна отправилась в Мордор за Дошираком, но, как назло, не было ни одного по акции. Но ребята не отчаялись, и смекнули, что легко достанут денег на заветную лапшу,если сделают выдающееся научное открытие и получат за него премию. В качестве открытия они (мы) выбрали обнаружение и исследование Йети с Марса. Никто и никогда их не видел. Более того - никто никогда не верил в то, что они могут существовать на самом деле. Представляете, что случится с научным миром, когда мы такого найдём?', E'\\x20D5', E'\\x1193', 100000, 3010, 1, false, null, null, 2);
INSERT INTO public.expedition (expedition_id, name, description, avatar_small, avatar_full, costs, current_sum, stage_id, banned, route_plan, route_current, head) VALUES (2, 'Забаненная экспедиция', 'Жаль,но эту экспедицию забанили', E'\\x0001', E'\\x11EE', 100000, 3000, 1, true, null, null, 3);
INSERT INTO public.expedition (expedition_id, name, description, avatar_small, avatar_full, costs, current_sum, stage_id, banned, route_plan, route_current, head) VALUES (3, 'Экспедиция по карьерной лестнице', 'Здравстсвуйте, ребятишки. В этом выпуске небылиц мы расскажем вам о том,как инвестировать в биткоин. Курс предполагает начальные знания в данной области,а именно: вы должны знать Python3 и уметь играть на валютных биржах как шулер со стажем более 30 лет', null, null, 350000, 0, 2, false, null, null, 1);
INSERT INTO public.expedition (expedition_id, name, description, avatar_small, avatar_full, costs, current_sum, stage_id, banned, route_plan, route_current, head) VALUES (4, 'Лень', 'Господи как же лень это придумывать', null, null, 1, 1, 3, false, null, null, 2);

