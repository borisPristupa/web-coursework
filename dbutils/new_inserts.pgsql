INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (1, 'Ложка', true, 'Весьма древняя ложка. Не слишком ценный артефакт, но, по крайней мере, мне пока что было не лень придумывать это описание. Уверен,дальше будет уже скучнее. Будет больше повторов и меньше оригинального контента. Прошу простить, но и понять - в условиях ограниченного времени и скукоёмкости написания описаний, меня вполне можно понять (по крайней мере,мне это пока что удавалось). На самом деле,наверное,на этом закончу сие повествование. Как говорится, сказочке конец, а кто дослушал - тот дослушал.', 4, '', '', 1, 6, 2, 48, 2, false);
INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (2, 'Нож', true, 'Нож. Если больше 10 см - это оружие. Острый. Иногда обоюдоострый. Вроде бы без крови. А, нет, погодите-ка...', 3, '', '', 8, 1, 2, 55, 2, true);
INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (3, 'Камень', false, 'ага, а ещё ножницы и бумага. Жаль мне было лень создавать такие артефакты', 1, '', '', 3, 8, 2, 63, 2, false);
INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (4, 'Жрец', true, 'Древнейшая профессия... Кхм, ну в смысле из мужских', 5, '', '', 4, 1, 2, 59, 3, false);
INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (5, 'Конопля', true, 'Вот раньше были огромные и насекомые,и растения. Наверное, было бы неплохо, имей конопля листья 2м в диаметре. Это ж можно было бы сделать сигару размером с руку. Это ж можно было бы накурить кита. Я,конечно,понимаю,что это незаконно, но! Не думаю что кит будет против. Да и люди никогда не видели китов навеселе, это бы точно потрясло научный мир. Так сказать, цели сугубо научные,а исследования категорически полезные и важные', 8, '', '', 1, 6, 2, 44, 1, false);
INSERT INTO public.artifact (artifact_id, name, approved, description, age_id, avatar_small, avatar_full, owner, approver, origin, country_id, category_id, banned) VALUES (6, 'Аниме', true, 'Конничива', 1, '', '', 2, 1, 2, 62, 3, false);

