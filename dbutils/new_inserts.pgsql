INSERT INTO public.human (human_id, login, password, email, vk_id, tg_nickname, first_name, second_name, last_name, bio, country_id, likes, dislikes, archaeologist, researcher, collector, sponsor, moderator, avatar_small, avatar_full, banned) VALUES (1, 'nursat', '555', 'aalena.hinda@buycow.org', null, 'nursat', 'Nursat', 'Baigenzheyev', '', 'Не самая правдоподобная автобиография, ну да что ж поделаешь - времена нынче трудные. На самом деле это даже не автобиография, а просто рыба. Самая первая рыба, которая пришла в голову. И хоть тут и много текста, смысла тут практически не заложено. Единственное "но" - это если Вы дочитали до этого момента, то уж извольте узнать, что однажды Пупа и Лупа пошли за зарплатой...', 44, 28, 3, false, true, true, false, true, '', '', false);
